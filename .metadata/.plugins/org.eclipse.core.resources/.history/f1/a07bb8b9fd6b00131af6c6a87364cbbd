public class Util {
	
	public static void main(String[] args) {
		
		List<Integer> a = new ArrayList<Integer>();
		a.add(1);
		a.add(2);
		a.add(3);
		a.add(4);
		a.add(5);
		
		List<String> l = new ArrayList<String>();
		
		
		for (int i = 0; i < a.size() - 1; i++) {
			
			for (int j = i+1; j < a.size(); j++) {
				
				String aux = a.get(i).concat(a.get(j));
				
			}
			
		}
		
	}
	

}
